grid_file = inlet1.grd                                         ! grid file
forcing_file = inlet1.bfr                                      ! forcing file
p = 3                                                          ! polynomial order
ctp = 1                                                        ! parametric coordinate transformation order
hbp = 1                                                        ! bathymetry order
rk = 22                                                        ! rk order
