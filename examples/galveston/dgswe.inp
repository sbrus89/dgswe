grid_file = galveston.grd                                                                            ! grid file
forcing_file = galveston.bfr                                                                         ! forcing file
bathy_file = galveston.hb                                                                            ! high order bathymetry file
curve_file = galveston.cb                                                                            ! curved boundary file
p = 3                                                                                                ! p - polynomial order
ctp = 3                                                                                              ! ctp - parametric coordinate transformation order
hbp = 3                                                                                              ! hbp - bathymetry order
rk = 22                                                                                              ! RK timestepping scheme
dt = 0.12d0                                                                                          ! dt - timestep (seconds)
tf = 4d0                                                                                             ! tf - final time (days)
dramp = .5d0                                                                                         ! dramp - ramping coefficient
cf = .003d0                                                                                          ! cf - friction coefficient
h0 = .2d0                                                                                            ! h0- minimum depth
sol_opt = 1                                                                                          ! sol_opt - solution output option
sol_snap = 48d0                                                                                      ! sol_snap - solution output interval
esl = .5d0                                                                                           ! esl - eddy viscosity parameter
cb_wmult = 6                                                                                         ! curved element decomposition weight
slam0 = -94.90
sphi0 = 29.195
